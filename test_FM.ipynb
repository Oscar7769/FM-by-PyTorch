   "source": [
    "# Generate a bqm with weights in [-1, 1]\n",
    "\n",
    "def custom_bias_generator(var_num):\n",
    "    return np.random.uniform(-1, 1, var_num)\n",
    "\n",
    "bqm = dimod.generators.gnm_random_bqm( # 用來生成一個隨機的BQM\n",
    "          variables = var_num,  #指定BQM有var_num個二元變數\n",
    "          num_interactions = var_num*(var_num-1)/2,  #指定二次項的數量\n",
    "          vartype = dimod.BINARY, #指定變數類型為二元\n",
    "          bias_generator=custom_bias_generator\n",
    "      )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "47a1768f-5898-4b3f-9b42-841e5f6d9138",
   "metadata": {},
   "outputs": [],
   "source": [
    "b = bqm.offset       # bias     -> type: 0-D numpy 偏移量\n",
    "h = bqm.linear       # linear   -> type: dict like 線性項\n",
    "Q = bqm.quadratic    # coupling -> type: dict like 二次項"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "0a66d14e-b9e5-4861-923a-250b2024fc4a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert the dict like data to ndarray 將字典格式的h和Q轉換為numpy陣列\n",
    "N = len(h)    # it should be identical to \"var_num\"\n",
    "h_arr = np.array([h[i] for i in range(N)], np.float64)\n",
    "Q_arr = np.zeros((N, N))\n",
    "for (i, j), value in Q.items():\n",
    "    Q_arr[i, j] = value\n",
    "Q_arr = Q_arr + Q_arr.T #dimod的Q字典只有儲存上三角部分，因此需要與自身轉置後相加\n",
    "np.fill_diagonal(Q_arr, h_arr)    # 將線性項h填入Q矩陣的對角線\n",
    "\n",
    "# 整個能量公式 E(x) 被簡潔地表示為 xᵀ * Q_arr * x + b"
   ]
